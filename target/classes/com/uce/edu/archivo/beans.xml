<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    
    <bean id="cuenta" class="com.ec.edu.inyeccion.dependencias.framework.Cuenta">
    	<property name="numero" value="54321"></property>
    	<property name="saldo" value="100"></property>
    </bean>
    
    <bean id="cuenta1" class="com.ec.edu.inyeccion.dependencias.framework.Cuenta">
    	<property name="numero" value="23"></property>
    	<property name="saldo" value="0"></property>
    </bean>
    
    <bean id="estu" class="com.ec.edu.inyeccion.dependencias.framework.Estudiante">
    	<constructor-arg value="Nicolas"></constructor-arg>
    	<constructor-arg value="Carrera"></constructor-arg>
    	<constructor-arg value="22"></constructor-arg>
    </bean>
    
    <bean id="paciete" class="nicolas.carrera.tarea.Paciente">
    	<constructor-arg value="Nicolas"></constructor-arg>
    	<constructor-arg value="Carrera"></constructor-arg>
    	<constructor-arg value="22"></constructor-arg>
    </bean>
    
    <bean id="profesor" class="nicolas.carrera.tarea.Profesorr">
    	<constructor-arg value="Edison"></constructor-arg>
    	<constructor-arg value="Cayambe"></constructor-arg>
    	<constructor-arg value="ING. Systemas"></constructor-arg>
    	<constructor-arg value="Universidad Central del Ecuador"></constructor-arg>
    </bean>
    
</beans>